  print(ANSI_RED,"  /_/-----/_/|",ANSI_YELLOW, "   __      ")
  print(ANSI_RED,"  ( ( ' ' ( (|",ANSI_YELLOW," /'--'\    ")
  print(ANSI_RED,"  (_( ' ' (_(|",ANSI_YELLOW,"/.    .\   ")
  print(ANSI_RED,"  / /=====/ /|",ANSI_YELLOW,"  '||'     ")
  print(ANSI_RED," /_//____/_/ |",ANSI_YELLOW,"   ||      ")
  print(ANSI_RED,"(o|:.....|o) |",ANSI_YELLOW,"   ||      ")
  print(ANSI_RED,"|_|:_____|_|/'",ANSI_YELLOW,"  _||_     ")
  print(ANSI_RED," '        '   ",ANSI_YELLOW," /____\    ")
  print(RESET_COLOR)
  print(" ")